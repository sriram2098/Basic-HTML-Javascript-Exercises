    <body>
        <h1>JAVA SCRIPT</h1>

    <script type="text/javascript">

            var even=[];
            var odd=[];
            for (var i=50; i<=100; i++)
            {
            if (i % 2 === 0)
                {
                    even.push(i);
                }
                else{
                    odd.push(i);
                }
            }
            console.log(even);
            console.log(odd);
            </script>

    </body>
            
